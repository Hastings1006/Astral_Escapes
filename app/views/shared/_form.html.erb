<%= simple_form_for [planet, booking],
    data: {
      insert_in_list_target: "form",
      action: "submit->insert-in-list#send"
    } do |f| %>
        <%= f.input :start_date,
        as: :string,
        input_html: { data: { controller: "datepicker" } } %>
        <%= f.input :end_date,
        as: :string,
        input_html: { data: { controller: "datepicker" } } %>
        <%= f.submit %>
      <% end %>
